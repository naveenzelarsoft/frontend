pipeline {
  agent any
    stages {
      stage ('Prepare Artifacts') {
        steps {
          sh '''
             zip -r frontend.zip *
          '''
          }
        }
     stage ('Upload Artifact') {
        steps {
        sh '''
          curl -f -v -u admin:admin --upload-file frontend.zip http://18.208.197.213:8081/repository/frontend/frontend.zip
         
           ''' 
          }
        }
     }
  }
