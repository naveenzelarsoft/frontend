pipeline {
  agent any
    stages {
      stage ('Prepare Artifacts') {
        steps {
          sh '''
             zip -r ../frontend.zip . -i *
          '''
          }
        }
     stage ('Upload Artifact')
        steps {
        sh '''
          curl -v -u admin:admin --upload-file frontend.zip http://34.200.251.226:8081/repository/frontend/frontend.zip
         
           ''' 
        }
     }
  }
